issn = {1520-5207},
journal = {The journal of physical chemistry. B},
keywords = {Amyloid beta-Peptides,Amyloid beta-Peptides: chemistry,Islet Amyloid Polypeptide,Islet Amyloid Polypeptide: chemistry,Molecular Dynamics Simulation,Peptide Fragments,Peptide Fragments: chemistry,Temperature,Water,Water: chemistry},
month = dec,
number = {48},
pages = {14217--28},
pmid = {21999345},
title = {{Volumetric properties of hydrated peptides: Voronoi-Delaunay analysis of molecular simulation runs.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21999345},
