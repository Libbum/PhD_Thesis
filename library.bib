issn = {0021-9991},
journal = {Journal of computational physics},
keywords = {pair distribution function,two point correlation function},
month = may,
number = {9},
pages = {3556--3569},
pmid = {21547007},
publisher = {Elsevier Inc.},
title = {{Fast Analysis of Molecular Dynamics Trajectories with Graphics Processing Units-Radial Distribution Function Histogramming.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3085256\&tool=pmcentrez\&rendertype=abstract},
