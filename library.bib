journal = {Physical Review B},
keywords = {f noise,flicker 1,josephson junctions,qubit,single electron transistors,small tunnel junctions},
month = oct,
number = {14},
pages = {144506},
title = {{Anomalous avoided level crossings in a Cooper-pair box spectrum}},
url = {http://arxiv.org/abs/0810.2455 http://link.aps.org/doi/10.1103/PhysRevB.78.144506},
volume = {78},
year = {2008}
