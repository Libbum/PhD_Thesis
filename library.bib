keywords = {Cholesterol,Cholesterol: chemistry,Dimyristoylphosphatidylcholine,Dimyristoylphosphatidylcholine: chemistry,Liposomes,Liposomes: chemistry,Models,Molecular,Molecular Conformation,Monte Carlo Method,Phospholipids,Phospholipids: chemistry,Solubility},
month = sep,
